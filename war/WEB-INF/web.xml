<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>SwingsetWeb</servlet-name>
		<servlet-class>com.jeremyhaberman.swingsetweb.SwingsetWebServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SwingsetWeb</servlet-name>
		<url-pattern>/swingsetweb</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Playground</servlet-name>
		<servlet-class>com.jeremyhaberman.swingsetweb.PlaygroundServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Playground</servlet-name>
		<url-pattern>/playground</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>playgrounds.jsp</welcome-file>
	</welcome-file-list>
</web-app>
